{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "lib",
    "alwaysStrict": true,
    "declaration": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": [
      "es2019",
      "dom"
    ],
    "module": "esnext",
    "noEmitOnError": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "target": "es6",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "preserve",
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "paths": {
      "@app/*": [
        "*"
      ]
    },
    "baseUrl": "./src",
    "useUnknownInCatchVariables": true
  },
  "include": [
    "src/**/*.ts",
    "src/global.d.ts"
  ],
  "exclude": [
    "build",
    "out_lambda",
    "next.config.js",
    "/home/runner/work/tnm-v4/tnm-v4/packages/infrastructure"
  ],
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}
